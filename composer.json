{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.262",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.2",
        "hirethunk/verbs": "^0.6.4",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.14",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^3.0",
        "predis/predis": "^2.1",
        "rappasoft/laravel-livewire-tables": "^3.2",
        "spatie/laravel-login-link": "^1.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-missing-page-redirector": "^2.9",
        "spatie/laravel-sitemap": "^6.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "peckphp/peck": "^0.1.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "rector/rector": "^1.2",
        "spatie/laravel-ignition": "^2.0",
        "tightenco/duster": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:type-coverage": "pest --type-coverage --min=55",
        "test:typos": "peck",
        "test:lint": "pint --test",
        "test:unit": "pest --parallel --coverage",
        "test:types": "phpstan",
        "test:refactor": "rector --dry-run",
        "test": [
            "@test:type-coverage",
            "@test:typos",
            "@test:unit",
            "@test:lint",
            "@test:refactor"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
